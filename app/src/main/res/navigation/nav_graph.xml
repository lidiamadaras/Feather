<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">


    <fragment
        android:id="@+id/dreamAnalysisFragment"
        android:name="com.example.feather.ui.dreamanalysis.DreamAnalysisFragment"
        android:label="fragment_dreamanalysis"
        tools:layout="@layout/fragment_dreamanalysis" >
    </fragment>
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.example.feather.ui.explore.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" >
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.feather.ui.home.HomeFragment"
        android:label="fragment_home3"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_logDreamFragment"
            app:destination="@id/logDreamFragment" />
        <action
            android:id="@+id/action_homeFragment_to_logFeelingFragment"
            app:destination="@id/logFeelingFragment" />
        <action
            android:id="@+id/action_homeFragment_to_reflectionFragment"
            app:destination="@id/reflectionFragment" />
        <action
            android:id="@+id/action_homeFragment_to_affirmationFragment"
            app:destination="@id/affirmationFragment" />
    </fragment>
    <fragment
        android:id="@+id/statsFragment"
        android:name="com.example.feather.ui.stats.StatsFragment"
        android:label="fragment_stats"
        tools:layout="@layout/fragment_stats" >
        <action
            android:id="@+id/action_statsFragment_to_myDreamsFragment"
            app:destination="@id/myDreamsFragment" />
        <action
            android:id="@+id/action_statsFragment_to_myFeelingsFragment"
            app:destination="@id/myFeelingsFragment" />
        <action
            android:id="@+id/action_statsFragment_to_myAffirmationsFragment"
            app:destination="@id/myAffirmationsFragment" />
        <action
            android:id="@+id/action_statsFragment_to_myReflectionsFragment"
            app:destination="@id/myReflectionsFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.feather.ui.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
    </fragment>


    <fragment
        android:id="@+id/logDreamFragment"
        android:name="com.example.feather.ui.log.LogDreamFragment"
        android:label="fragment_log_dream"
        tools:layout="@layout/fragment_log_dream" >
    </fragment>
    <fragment
        android:id="@+id/logFeelingFragment"
        android:name="com.example.feather.ui.log.LogFeelingFragment"
        android:label="fragment_log_feeling"
        tools:layout="@layout/fragment_log_feeling" >
    </fragment>
    <fragment
        android:id="@+id/reflectionFragment"
        android:name="com.example.feather.ui.log.ReflectionFragment"
        android:label="fragment_reflection"
        tools:layout="@layout/fragment_reflection" >
    </fragment>
    <fragment
        android:id="@+id/affirmationFragment"
        android:name="com.example.feather.ui.log.AffirmationFragment"
        android:label="fragment_affirmation"
        tools:layout="@layout/fragment_affirmation" >
    </fragment>


    <fragment
        android:id="@+id/myDreamsFragment"
        android:name="com.example.feather.ui.log.mylogs.MyDreamsFragment"
        android:label="fragment_my_dreams"
        tools:layout="@layout/fragment_my_dreams" >
        <action
            android:id="@+id/action_myDreamsFragment_to_dreamDetailFragment"
            app:destination="@id/dreamDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/myFeelingsFragment"
        android:name="com.example.feather.ui.log.mylogs.MyFeelingsFragment"
        android:label="fragment_my_feelings"
        tools:layout="@layout/fragment_my_feelings" >
        <action
            android:id="@+id/action_myFeelingsFragment_to_feelingDetailFragment"
            app:destination="@id/feelingDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/myAffirmationsFragment"
        android:name="com.example.feather.ui.log.mylogs.MyAffirmationsFragment"
        android:label="fragment_my_affirmations"
        tools:layout="@layout/fragment_my_affirmations" >
        <action
            android:id="@+id/action_myAffirmationsFragment_to_affirmationDetailFragment"
            app:destination="@id/affirmationDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/myReflectionsFragment"
        android:name="com.example.feather.ui.log.mylogs.MyReflectionsFragment"
        android:label="fragment_my_reflections"
        tools:layout="@layout/fragment_my_reflections" >
        <action
            android:id="@+id/action_myReflectionsFragment_to_reflectionDetailFragment"
            app:destination="@id/reflectionDetailFragment" />
    </fragment>




    <fragment
        android:id="@+id/dreamDetailFragment"
        android:name="com.example.feather.ui.log.mylogs.details.DreamDetailFragment"
        android:label="fragment_dream_details"
        tools:layout="@layout/fragment_dream_detail" >
    </fragment>
    <fragment
        android:id="@+id/feelingDetailFragment"
        android:name="com.example.feather.ui.log.mylogs.details.FeelingDetailFragment"
        android:label="fragment_feeling_details"
        tools:layout="@layout/fragment_feeling_detail" >
    </fragment>
    <fragment
        android:id="@+id/affirmationDetailFragment"
        android:name="com.example.feather.ui.log.mylogs.details.AffirmationDetailFragment"
        android:label="fragment_affirmation_details"
        tools:layout="@layout/fragment_affirmation_detail" >
    </fragment>

    <fragment
        android:id="@+id/reflectionDetailFragment"
        android:name="com.example.feather.ui.log.mylogs.details.ReflectionDetailFragment"
        android:label="fragment_reflection_details"
        tools:layout="@layout/fragment_reflection_detail" >
    </fragment>




</navigation>
